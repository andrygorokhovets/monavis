- /*Template Name:Creative Group */
doctype html
html
    head
        - get_header();
    body
        section.slider.head-slider
            include /mixins/slider.jade
        section.header
            include /mixins/header.jade
        section.content
            .container
                .row
                    .col-md-12.main-content
                        include /mixins/page-head.jade
                    .col-md-12.page-line
                        .page-container.pull-left

                            .page-heading
                                h3
                                    - print(get_the_title());
                            .page-content
                                .page-text
                                    p
                                        - print(get_the_content());
                                        - wp_reset_query();
                                        - query_posts( array ( 'post_type' => 'team'));
                                        - if (have_posts()):
                                        - while (have_posts()):
                                        - the_post();
                                        include /mixins/creative-group-block.jade
                                        - endwhile;
                                        - endif;
                                        - wp_reset_query();
        - get_footer();